{
  "manifest_version": 2,

  "name": "CG Spunk",
  "description": "Adds new functionality to the IDE for multiplayer games",
  "version": "0.0.2",

  "permissions": [
      "storage"
  ],

  "content_scripts": [
    {
      "matches": [ "https://www.codingame.com/*" ],
      "js": [ "jquery.js", "modIde.js" ]
    }
  ],

  "icons": {
    "16": "cg_16_16.png",
    "32": "cg_32_32.png"
  },
  
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
